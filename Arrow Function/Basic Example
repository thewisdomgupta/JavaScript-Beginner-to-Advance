// Arrow Function

const sumOfTwo = (num1, num2) =>{
    return num1+ num2;
}

const value = sumOfTwo(55, 887);
console.log(value);

const isEven = (number) =>{
    return number % 2===0;
}

console.log(isEven(46));  //Output -> True
console.log(isEven(45));  //Output -> False

